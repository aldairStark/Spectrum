<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.spectrum">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Spectrum">
        <activity android:name=".ui.activity_AsyncTask"></activity>
        <activity android:name=".ui.BackoutFragments" />
        <activity android:name=".ui.Peliculas" />
        <activity android:name=".ui.RecyclerView" />
        <activity android:name=".ui.ListView" />
        <activity android:name=".ui.interface2" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>